{
  "name": "zk-react",
  "private": true,
  "version": "0.8.0",
  "scripts": {
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm-run-all -c lint:code lint:styles lint:ts lint:html",
    "lint:code": "eslint \"src/**/*.ts*\"",
    "lint:ts": "tsc",
    "lint:fix": "npm-run-all -c \"lint:code --fix\" \"lint:styles --fix\" \"lint:html --write\" || exit 0",
    "lint:styles": "stylelint \"src/**/*.*css\"",
    "lint:html": "prettier index.html -c",
    "tsc": "tsc"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "effector": "^22.8.6",
    "effector-react": "^22.5.3",
    "framerate-react": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tiny-popover": "^7.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
